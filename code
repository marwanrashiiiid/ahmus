def handle_command(command):
    """
    Handle user commands with preprocessing to handle variations in input.
    """
    command = preprocess_text(command)
    if command in intents:
        return intents[command]
    else:
        return "عذرًا، لم أتمكن من معالجة طلبك."

def preprocess_text(text):
    """
    Preprocess the text by removing diacritics and special characters.
    """
    text = re.sub(r'[^\w\s]', '', text)
    return text.strip()

def save_learning(question, answer):
    """Save a new question-answer pair."""
    with open('learnings.txt', 'a') as file:
        file.write(f"Question: {question}\n")
        file.write(f"Answer: {answer}\n\n")

class AhmusApp(QWidget):
    def __init__(self):
        super().__init__()
        self.initUI()

    def initUI(self):
        self.setWindowTitle('Ahmus - مساعد الحضارة الفرعونية')
        self.layout = QVBoxLayout()

        self.label = QLabel('مرحبًا بك في Ahmus! اسألني عن الحضارة الفرعونية.')
        self.layout.addWidget(self.label)

        self.textEdit = QTextEdit()
        self.layout.addWidget(self.textEdit)

        self.commandInput = QLineEdit(self)
        self.commandInput.setPlaceholderText('اكتب سؤالك هنا...')
        self.layout.addWidget(self.commandInput)

        self.askButton = QPushButton('اسأل', self)
        self.askButton.clicked.connect(self.handle_ask)
        self.layout.addWidget(self.askButton)

        self.teachButton = QPushButton('علمني', self)
        self.teachButton.clicked.connect(self.handle_teach)
        self.layout.addWidget(self.teachButton)

        self.setLayout(self.layout)

    def handle_ask(self):
        command = self.commandInput.text()
        if command:
            response = handle_command(command)
            if isinstance(response, list) and len(response) > 0:
                response = ' '.join(response)  # Join the list elements into a single string
                response = response.replace("'", "")  # Remove single quotation marks
            self.textEdit.append(f'أنت: {command}')
            self.textEdit.append(f'Ahmus: {response}')
            self.commandInput.clear()
        else:
            QMessageBox.warning(self, 'تحذير', 'الرجاء كتابة سؤال قبل الضغط على زر "اسأل".')

    def handle_teach(self):
        question, ok = QInputDialog.getText(self, 'تعليم Ahmus', 'اكتب السؤال الذي تريد تعليمه:')
        if ok and question:
            answer, ok = QInputDialog.getText(self, 'تعليم Ahmus', 'اكتب الرد المناسب للسؤال:')
            if ok and answer:
                save_learning(question, answer)
                QMessageBox.information(self, 'نجاح', 'تم حفظ السؤال والرد بنجاح!')

def main():
    app = QApplication(sys.argv)
    ex = AhmusApp()
    ex.show()
    sys.exit(app.exec_())

if __name__ == '__main__':
    main()
